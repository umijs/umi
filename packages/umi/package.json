{
  "name": "umi",
  "version": "4.0.0-beta.1",
  "description": "umi",
  "main": "dist/index.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts",
    "index.esm.js",
    "client",
    "bin"
  ],
