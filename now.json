{
  "name": "umijs.org",
  "version": 2,
  "builds": [
    {
      "src": "./website/package.json",
      "use": "@now/static-build",
      "config": {
        "includeFiles": "docs/**",
        "distDir": "./dist"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "website/$1" }],
  "github": {
    "silent": true
  },
  "alias": ["umijs.now.sh", "umijs.org"]
}
