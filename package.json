{
  "private": true,
  "scripts": {
    "bootstrap": "umi-scripts bootstrap",
    "build": "umi-scripts turbo build",
    "build:extra": "umi-scripts turbo build:extra",
    "build:force": "umi-scripts turbo build --no-cache",
    "build:release": "umi-scripts turbo build --no-cache",
    "check:packageFiles": "umi-scripts checkPackageFiles",
    "codemod:build": "pnpm --filter @umijs/codemod build",
    "codemod:dev": "pnpm --filter @umijs/codemod build --watch",
    "codemod:jest": "cd codemod && jest",
    "codemod:release": "umi-scripts releaseCodemod",
    "codemod:run": "BACON=none GIT_CHECK=none ./codemod/bin/umi-codemod.js --cwd codemod/fixtures/tmp",
    "codemod:test": "BACON=none GIT_CHECK=none umi-scripts testCodemod",
    "cov": "jest --coverage",
    "dep:update": "pnpm up --interactive --latest --recursive",
    "dev": "umi-scripts turbo dev --parallel",
    "didyouknow:build": "pnpm --filter @umijs/did-you-know build",
    "didyouknow:dev": "pnpm --filter @umijs/did-you-know dev",
    "didyouknow:release": "umi-scripts releaseDidYouKnow",
    "doc:build": "pnpm doc:deps && umi build",
    "doc:deps": "pnpm doc:deps-ts && pnpm doc:deps-extra",
    "doc:deps-extra": "umi-scripts turbo build:extra --filter @umijs/plugin-docs...",
    "doc:deps-ts": "umi-scripts turbo build --filter @umijs/plugin-docs...",
    "doc:dev": "umi dev",
    "format": "prettier --cache --write .",
    "format:docs": "prettier --cache 'docs/**/*.{md,mdx}' --write --ignore-path .gitignore --ignore-unknown",
    "format:plugin-docs": "prettier --cache 'packages/plugin-docs/**/*' --write --ignore-unknown",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "umi-scripts postinstall",
    "jest": "jest",
    "prepare": "husky install",
    "release": "umi-scripts release",
    "setup:webstorm": "umi-scripts setupWebStorm",
    "sync:tnpm": "umi-scripts syncTnpm",
    "test": "jest",
    "test:clean": "umi-scripts turbo test --no-cache --parallel -- --clearCache",
    "tsc:check": "tsc --noEmit",
    "turbo:clean": "rimraf .turbo",
    "valtio:build": "pnpm --filter @umijs/valtio build",
    "valtio:dev": "pnpm --filter @umijs/valtio dev",
    "valtio:release": "umi-scripts releaseValtio"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --no-error-on-unmatched-pattern --cache --write"
    ],
    "*.ts?(x)": [
      "prettier --no-error-on-unmatched-pattern --cache --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@iconify-json/ant-design": "1",
    "@iconify-json/fa": "1",
    "@playwright/test": "^1.30.0",
    "@pnpm/lockfile-file": "^5.0.3",
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/resolve": "^1.20.2",
    "@types/rimraf": "3.0.2",
    "@umijs/bundler-esbuild": "workspace:*",
    "@umijs/bundler-utils": "workspace:*",
    "@umijs/bundler-vite": "workspace:*",
    "@umijs/bundler-webpack": "workspace:*",
    "@umijs/lint": "workspace:*",
    "@umijs/plugin-docs": "workspace:*",
    "@umijs/plugins": "workspace:*",
    "@umijs/server": "workspace:*",
    "@umijs/utils": "workspace:*",
    "@vercel/ncc": "0.33.3",
    "cross-env": "^7.0.3",
    "dts-packer": "^0.0.3",
    "father": "^4.1.3",
    "git-repo-info": "^2.1.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lerna": "^6.4.0",
    "lint-staged": "^13.1.0",
    "matcher": "^5.0.0",
    "octokit": "^2.0.11",
    "only-allow": "^1.1.0",
    "open": "^8.4.0",
    "prettier": "^2.8.2",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "qs": "^6.11.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-text-loop-next": "0.0.3",
    "regenerator-runtime": "^0.13.11",
    "resolve": "^1.22.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.15.3",
    "ts-node": "^10.7.0",
    "tsx": "^3.12.2",
    "turbo": "^1.7.0",
    "typescript": "^4.9.4",
    "uglify-js": "^3.17.4",
    "umi": "workspace:*",
    "umi-scripts": "workspace:*",
    "zx": "^7.1.1"
  },
  "packageManager": "pnpm@7.17.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "antd",
        "dva",
        "postcss",
        "webpack",
        "eslint",
        "stylelint",
        "redux"
      ]
    }
  },
  "_local": "This flag is used to check if the development in local, Please do not delete."
}
