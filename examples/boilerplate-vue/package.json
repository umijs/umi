    "preview": "max preview --port 6070",
    "setup": "umi setup",
    "start": "npm run dev"
  },
  "dependencies": {
    "@umijs/preset-vue": "4.0.50",
    "umi": "4.0.50",
    "vue": "3.2.45"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "cypress": "^10.0.0",
    "start-server-and-test": "^1.15.2"
  }
}
