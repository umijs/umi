    "koa-static": "^5.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "serialize-javascript": "^1.7.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.11",
    "concurrently": "^4.1.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "koa-proxy": "^0.9.0",
    "lint-staged": "^7.2.2",
