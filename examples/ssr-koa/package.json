    "umi": "^2.4.2",
    "umi-plugin-react": "^1.4.0"
  },
  "engines": {
    "install-alinode": "3.11.7"
  },
  "scripts": {
    "prod": "rm -rf dist && concurrently \"USE_SSR=true npm run build:csr && npm run build:ssr\" \"node ./app.js\"",
    "ssr": "concurrently \"NODE_ENV=development nodemon ./app.js\" \"npm run watch \"",
    "csr": "HOST=localhost umi dev",
    "build:csr": "umi build",
    "test:umi": "umi test",
    "lint:umi": "eslint --ext .js src mock tests",
    "build:ssr": "umi build --ssr",
    "watch": "umi build --ssr --watch",
    "precommit": "lint-staged",
    "start": "rm -rf dist && concurrently \"npm run ssr\" \"USE_SSR=true npm run csr\""
  },
